{
  "name": "microgen",
  "version": "1.1.3",
  "description": "micro-generator for individual files, easy like sunday morning",
  "bin": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/tap test/index.js --coverage-report=text",
