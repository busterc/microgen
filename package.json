    "plop",
    "templates",
    "template",
    "micro-generator"
  ],
  "author": "Buster Collings <busterc@gmail.com> (https://about.me/buster)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/busterc/microgen/issues"
  },
  "homepage": "https://github.com/busterc/microgen#readme",
  "dependencies": {
    "fs-extra": "^4.0.2",
    "handlebars": "^4.0.11"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-xo-space": "^0.17.0",
    "tap": "^10.7.3"
